;\n};\n\nconst formSubmit = () => {\n  const newUser = {\n    fname: formValues.fname.trim(),\n    lname: formValues.lname.trim(),\n    email: formValues.email.trim(),\n    password: formValues.password.trim(),\n    role: formValues.role.trim(),\n    terms: formValues.terms\n  }\n\n  postNewUser(newUser);\n  \n}\n\n// useEffect(() => {\n//   getUsers();\n// }, []);\n\nuseEffect(() => {\n  formSchema.isValid(formValues).then((valid) => {\n    setDisabled(!valid);\n  })\n}, [formValues]);\n\n  return (\n    <div className=\"App\">\n     <header>\n       <h1>Form Application App</h1>\n     </header>\n\n     <Form\n      values={formValues}\n      update={inputChange}\n      submit={formSubmit}\n      disabled={disabled}\n      errors={formErrors}\n      />\n\n      {users.map((user) => {\n        return <User key={user.id} userInfo={user} />;\n      })}\n    </div>\n  );\n}\n\nexport default App;\n","C:\\Users\\Kenan Dolic\\Documents\\LambdaCode\\Unit2\\User-Onboarding\\user-onboarding\\src\\components\\User.js",[],"C:\\Users\\Kenan Dolic\\Documents\\LambdaCode\\Unit2\\User-Onboarding\\user-onboarding\\src\\components\\Form.js",[],"C:\\Users\\Kenan Dolic\\Documents\\LambdaCode\\Unit2\\User-Onboarding\\user-onboarding\\src\\validation\\formSchema.js",[],["32","33"],{"ruleId":"34","replacedBy":"35"},{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","severity":1,"message":"39","line":37,"column":7,"nodeType":"40","messageId":"41","endLine":37,"endColumn":15},{"ruleId":"34","replacedBy":"42"},{"ruleId":"36","replacedBy":"43"},"no-native-reassign",["44"],"no-negated-in-lhs",["45"],"no-unused-vars","'getUsers' is assigned a value but never used.","Identifier","unusedVar",["44"],["45"],"no-global-assign","no-unsafe-negation"]